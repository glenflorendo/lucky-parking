{
  "$schema": "https://developer.microsoft.com/json-schemas/rush/v5/rush.schema.json",
  "rushVersion": "5.119.0",
  "pnpmVersion": "8.15.5",
  "nodeSupportedVersionRange": ">=20.12.0 <21.0.0",
   "suppressNodeLtsWarning": false,
   "ensureConsistentVersions": true,
   "projectFolderMinDepth": 2,
   "projectFolderMaxDepth": 2,
   "allowMostlyStandardPackageNames": false,
   "approvedPackagesPolicy": {
     "reviewCategories": [
       "application",
       "library",
       "prototype",
       "tool"
     ],
      "ignoredNpmScopes": ["@types"]
   },
  "gitPolicy": {
     "versionBumpCommitMessage": "chore: bump versions [skip ci]",
     "changeLogUpdateCommitMessage": "docs: update changelogs [skip ci]",
      "changefilesCommitMessage": "chore: rush change"
  },
  "repository": {
     "url": "https://github.com/hackforla/lucky-parking",
     "defaultBranch": "main",
     "defaultRemote": "origin"
  },
  "eventHooks": {
    "preRushInstall": [],
    "postRushInstall": [],
    "preRushBuild": [],
    "postRushBuild": [],
    "preRushx": [],
    "postRushx": []
  },
   "telemetryEnabled": false,
   "hotfixChangeEnabled": false,
   "allowedProjectTags": [],
  "projects": []
}
